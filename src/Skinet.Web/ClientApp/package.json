{
  "name": "skinet",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44422 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44422 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "config": "ts-node src/environments/set-env.ts",
    "build": "npm run config && ng build",
    "build:ssr": "ng run Skinet:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc:build": "compodoc -p tsconfig.doc.json -d ./doc -n Skinet --theme postmark",
    "compodoc:serve": "npm run compodoc:build && compodoc -s -d ./doc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/platform-server": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@compodoc/compodoc": "^1.1.19",
    "@paralleldrive/cuid2": "^2.2.0",
    "@stripe/stripe-js": "^1.52.1",
    "bootswatch": "^5.2.3",
    "dotenv": "^16.0.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "ngx-spinner": "^15.0.1",
    "ngx-toastr": "^16.1.1",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "run-script-os": "^1.1.6",
    "tslib": "^2.4.0",
    "xng-breadcrumb": "^9.0.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.7",
    "@angular/cli": "^15.2.7",
    "@angular/compiler-cli": "^15.2.8",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.0.0",
    "bootstrap": "^5.2.3",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ngx-bootstrap": "^10.2.0",
    "rxjs": "^7.4.0",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
